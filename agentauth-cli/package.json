{
  "name": "@umytbaynazarow/agentauth-cli",
  "version": "0.1.0",
  "description": "CLI tool for AgentAuth - Setup, test, and deploy agent authentication in minutes",
  "main": "dist/index.js",
  "bin": {
    "agentauth": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsx src/index.ts",
    "test": "tsx src/index.ts test",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "agentauth",
    "authentication",
    "ai-agents",
    "cli",
    "setup"
  ],
  "author": "umytbaynazarov-coder",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "dotenv": "^16.4.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
